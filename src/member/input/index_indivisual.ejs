<%
var pageData = {
	path: '../../',
	title: '',
	keywords: '',
	description: '',
	pnkzlabel: '新規会員申請'
};
%>
<!doctype html>
	<html lang="ja">
		<head>
			<%- include( '../../includes/_head', { data: pageData } ) %>
		</head>
	
		<body>
			<%- include( '../../includes/_header', { data: pageData } ) %>
			<div class="container registration">
				<h2 class="title en">Registration</h2>
				<%- include( '../../includes/_breadcrumb', { data: pageData } ) %>
				<div class="wrapper">
					<div class="single_content">
						<form action="" class="registration">
							<div class="row">
								<div class="row_head">
									<p>法人／個人<span>※</span></p>
									<p class="note"><span>※</span>は必須項目</p>
								</div>
								<div class="row_body">
									<div class="row_item">
										<input type="radio" class="form_radio" name="category" id="category_01">
										<label for="category_01" class="form_label">法人</label>
									</div>
									<div class="row_item">
										<input type="radio" class="form_radio" name="category" id="category_02" checked>
										<label for="category_02" class="form_label">個人</label>
									</div>
								</div>
								
							</div>
							<div class="row is-hidden">
								<div class="row_head">
									<p>氏名<span>※</span></p>
								</div>
								<div class="row_group">
									<div class="row_item_full">
										<input type="text" placeholder="例：山田 太郎" class="form_input">
									</div>
								</div>
								
							</div>
							<div class="row">
